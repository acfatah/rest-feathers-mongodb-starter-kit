{
  "name": "rest-feathers-mongodb-starter-kit",
  "description": "RESTful API starter kit using Feathers and Mongodb.",
  "version": "0.0.0",
  "homepage": "https://github.com/acfatah/rest-feathers-mongodb-starter-kit",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Achmad F. Ibrahim",
    "email": "acfatah@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^12.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run eslint && npm run jest",
    "eslint": "eslint src/. test/. --config .eslintrc.json",
    "build": "cross-env NODE_ENV=production babel src -d dist",
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node src/",
    "start": "npm run build && cross-env NODE_ENV=production node dist/",
    "jest": "cross-env NODE_ENV=testing jest --forceExit"
  },
  "dependencies": {
    "@feathersjs/configuration": "^4.3.0",
    "@feathersjs/errors": "^4.3.0",
    "@feathersjs/express": "^4.3.0",
    "@feathersjs/feathers": "^4.3.0",
    "@feathersjs/socketio": "^4.3.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "feathers-mongodb": "^5.0.0",
    "helmet": "^3.21.0",
    "serve-favicon": "^2.5.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "axios": "^0.19.0",
    "cross-env": "^6.0.0",
    "eslint": "^6.3.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.2"
  }
}
